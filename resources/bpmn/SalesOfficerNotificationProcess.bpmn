      <bpmn:sequenceFlow id="Flow_deadline_to_close" sourceRef="Timer_Deadline" targetRef="Task_SendClosing" />
      <bpmn:sequenceFlow id="Flow_close_to_subend" sourceRef="Task_SendClosing" targetRef="SubEnd_NoResponse" />
      <bpmn:sequenceFlow id="Flow_schedule_to_timer_wait" sourceRef="Task_CalcNextWindow" targetRef="Timer_WaitForWindow" />
      <bpmn:sequenceFlow id="Flow_gw_initial_schedule" name="SCHEDULE" sourceRef="GW_Initial" targetRef="Task_CalcNextWindow">
        <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${dmnResult.action == 'SCHEDULE_NEXT_WINDOW'}</bpmn:conditionExpression>
      </bpmn:sequenceFlow>
      <bpmn:scriptTask id="Task_CalcNextWindow" name="Calculate Next Business Window" scriptFormat="javascript" camunda:resultVariable="nextWindowDateTime">
        <bpmn:incoming>Flow_gw_initial_schedule</bpmn:incoming>
        <bpmn:outgoing>Flow_schedule_to_timer_wait</bpmn:outgoing>
        <bpmn:script></bpmn:script>
      </bpmn:scriptTask>
