<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="Definitions_notification_process" targetNamespace="http://camunda.org/schema/1.0/bpmn" exporter="Camunda Modeler" exporterVersion="5.44.0">
  <bpmn:process id="SalesOfficerNotificationProcess" name="Sales Officer Notification &#38; Follow-ups Protocol" isExecutable="true" camunda:historyTimeToLive="180">
    <bpmn:startEvent id="StartEvent_Notification" name="Notification Triggered">
      <bpmn:outgoing>Flow_start_to_sub</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:subProcess id="SubProcess_NotificationFlow" name="Notification Flow">
      <bpmn:incoming>Flow_start_to_sub</bpmn:incoming>
      <bpmn:outgoing>Flow_sub_to_set_closed</bpmn:outgoing>
      <bpmn:startEvent id="SubStart" name="Start Notification Flow">
        <bpmn:outgoing>Flow_sub_start_to_calc_bh</bpmn:outgoing>
      </bpmn:startEvent>
      <bpmn:scriptTask id="Task_CalcBH_Initial" name="Calculate Business Hours" scriptFormat="javascript" camunda:resultVariable="isBusinessHours">
        <bpmn:incoming>Flow_sub_start_to_calc_bh</bpmn:incoming>
        <bpmn:outgoing>Flow_calc_to_dmn_initial</bpmn:outgoing>
        <bpmn:script>
var now = new java.util.Date();
var cal = java.util.Calendar.getInstance();
cal.setTime(now);
var dow = cal.get(java.util.Calendar.DAY_OF_WEEK);
var hour = cal.get(java.util.Calendar.HOUR_OF_DAY);
var isWeekday = (dow &gt;= java.util.Calendar.MONDAY &amp;&amp; dow &lt;= java.util.Calendar.FRIDAY);
var inWindow = (hour &gt;= 6 &amp;&amp; hour &lt; 22);
isWeekday &amp;&amp; inWindow;
        </bpmn:script>
      </bpmn:scriptTask>
      <bpmn:businessRuleTask id="DMN_Initial" name="Evaluate Notification Action (INITIAL)" camunda:resultVariable="dmnResult" camunda:decisionRef="SalesOfficerNotificationAction" camunda:mapDecisionResult="singleResult">
        <bpmn:extensionElements>
          <camunda:inputOutput>
            <camunda:inputParameter name="notificationStage">INITIAL</camunda:inputParameter>
            <camunda:inputParameter name="responseReceived">${false}</camunda:inputParameter>
          </camunda:inputOutput>
        </bpmn:extensionElements>
        <bpmn:incoming>Flow_calc_to_dmn_initial</bpmn:incoming>
        <bpmn:outgoing>Flow_dmn_initial_to_gw</bpmn:outgoing>
      </bpmn:businessRuleTask>
      <bpmn:exclusiveGateway id="GW_Initial" name="Initial Action?">
        <bpmn:incoming>Flow_dmn_initial_to_gw</bpmn:incoming>
        <bpmn:outgoing>Flow_gw_initial_send</bpmn:outgoing>
        <bpmn:outgoing>Flow_gw_initial_schedule</bpmn:outgoing>
      </bpmn:exclusiveGateway>
      <bpmn:intermediateCatchEvent id="Timer_WaitForWindow" name="Wait for Business Window">
        <bpmn:incoming>Flow_schedule_to_timer_wait</bpmn:incoming>
        <bpmn:outgoing>Flow_timer_wait_to_send_initial</bpmn:outgoing>
        <bpmn:timerEventDefinition id="TimerDef_WaitWindow">
          <bpmn:timeDate xsi:type="bpmn:tFormalExpression">${nextWindowDateTime}</bpmn:timeDate>
        </bpmn:timerEventDefinition>
      </bpmn:intermediateCatchEvent>
      <bpmn:exclusiveGateway id="GW_Merge_Initial" name="Merge">
        <bpmn:incoming>Flow_gw_initial_send</bpmn:incoming>
        <bpmn:incoming>Flow_timer_wait_to_send_initial</bpmn:incoming>
        <bpmn:outgoing>Flow_merge_to_send_initial</bpmn:outgoing>
      </bpmn:exclusiveGateway>
      <bpmn:serviceTask id="Task_SendInitial" name="Send Initial Notification" camunda:type="external" camunda:topic="sendNotification">
        <bpmn:extensionElements>
          <camunda:inputOutput>
            <camunda:inputParameter name="notificationType">INITIAL_ALERT</camunda:inputParameter>
          </camunda:inputOutput>
        </bpmn:extensionElements>
        <bpmn:incoming>Flow_merge_to_send_initial</bpmn:incoming>
        <bpmn:outgoing>Flow_send_initial_to_timer_fu1</bpmn:outgoing>
      </bpmn:serviceTask>
      <bpmn:intermediateCatchEvent id="Timer_FU1" name="Wait 1 Hour">
        <bpmn:incoming>Flow_send_initial_to_timer_fu1</bpmn:incoming>
        <bpmn:outgoing>Flow_timer_fu1_to_send</bpmn:outgoing>
        <bpmn:timerEventDefinition id="TimerDef_FU1">
          <bpmn:timeDuration xsi:type="bpmn:tFormalExpression">PT1H</bpmn:timeDuration>
        </bpmn:timerEventDefinition>
      </bpmn:intermediateCatchEvent>
      <bpmn:serviceTask id="Task_SendFU1" name="Send Follow-Up 1" camunda:type="external" camunda:topic="sendNotification">
        <bpmn:extensionElements>
          <camunda:inputOutput>
            <camunda:inputParameter name="notificationType">FOLLOW_UP_1</camunda:inputParameter>
          </camunda:inputOutput>
        </bpmn:extensionElements>
        <bpmn:incoming>Flow_timer_fu1_to_send</bpmn:incoming>
        <bpmn:outgoing>Flow_send_fu1_to_timer_fu2</bpmn:outgoing>
      </bpmn:serviceTask>
      <bpmn:intermediateCatchEvent id="Timer_FU2" name="Wait 3 Hours">
        <bpmn:incoming>Flow_send_fu1_to_timer_fu2</bpmn:incoming>
        <bpmn:outgoing>Flow_timer_fu2_to_send</bpmn:outgoing>
        <bpmn:timerEventDefinition id="TimerDef_FU2">
          <bpmn:timeDuration xsi:type="bpmn:tFormalExpression">PT3H</bpmn:timeDuration>
        </bpmn:timerEventDefinition>
      </bpmn:intermediateCatchEvent>
      <bpmn:serviceTask id="Task_SendFU2" name="Send Follow-Up 2" camunda:type="external" camunda:topic="sendNotification">
        <bpmn:extensionElements>
          <camunda:inputOutput>
            <camunda:inputParameter name="notificationType">FOLLOW_UP_2</camunda:inputParameter>
          </camunda:inputOutput>
        </bpmn:extensionElements>
        <bpmn:incoming>Flow_timer_fu2_to_send</bpmn:incoming>
        <bpmn:outgoing>Flow_send_fu2_to_timer_deadline</bpmn:outgoing>
      </bpmn:serviceTask>
      <bpmn:scriptTask id="Task_CalcDeadline" name="Calculate 16h Business-Hour Deadline" scriptFormat="javascript" camunda:resultVariable="deadlineDateTime">
        <bpmn:incoming>Flow_send_fu2_to_timer_deadline</bpmn:incoming>
        <bpmn:outgoing>Flow_calc_deadline_to_timer</bpmn:outgoing>
        <bpmn:script>
var cal = java.util.Calendar.getInstance();
var remainingHours = 16;
while (remainingHours &gt; 0) {
  var dow = cal.get(java.util.Calendar.DAY_OF_WEEK);
  var hour = cal.get(java.util.Calendar.HOUR_OF_DAY);
  var isWeekday = (dow &gt;= java.util.Calendar.MONDAY &amp;&amp; dow &lt;= java.util.Calendar.FRIDAY);
  if (isWeekday &amp;&amp; hour &gt;= 6 &amp;&amp; hour &lt; 22) {
    remainingHours--;
  }
  cal.add(java.util.Calendar.HOUR_OF_DAY, 1);
}
var sdf = new java.text.SimpleDateFormat("yyyy-MM-dd'T'HH:mm:ss");
sdf.format(cal.getTime());
        </bpmn:script>
      </bpmn:scriptTask>
      <bpmn:intermediateCatchEvent id="Timer_Deadline" name="Wait for 16h Deadline">
